# LibrerÃ­as
library(tidyverse)

# Cargar datos
datos <- read_csv("00_apoyo/BD/superstore_dataset2011-2015.csv")

# Generar duplicados
duplicados <- sample_n(datos, size =round(nrow(datos) * 0.04422), replace = T)

# Pegar duplicados

datos_vf <- rbind(datos, duplicados)

write.csv(datos_vf, file = "datos/eval_05_superstore_11_15.csv", fileEncoding = "UTF-8")
warnings()
