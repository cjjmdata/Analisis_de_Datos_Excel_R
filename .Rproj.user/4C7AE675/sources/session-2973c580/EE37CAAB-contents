# LibrerÃ­as
library(tidyverse)

# Cargar datos
datos <- read.csv("00_apoyo/BD/superstore_dataset2011-2015_vf.csv")

# Generar duplicados
duplicados <- sample_n(datos, size =round(nrow(datos) * 0.04422), replace = T)

# Pegar duplicados

datos_vf <- rbind(datos, duplicados)

datos_vf2 <- datos_vf[sample(nrow(datos_vf)),]

write.csv(datos_vf2, file = "datos/eval_05_superstore_11_15.csv", row.names = FALSE)

